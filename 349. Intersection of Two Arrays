
     bool map[1001] = {false};    
    bool added[1001] = {false};  
    int* result = (int*)malloc(sizeof(int) * (nums1Size < nums2Size ? nums1Size : nums2Size));
    *returnSize = 0;
    for (int i = 0; i < nums1Size; i++) {
        map[nums1[i]] = true;
    }
    for (int i = 0; i < nums2Size; i++) {
        int val = nums2[i];
        if (map[val] && !added[val]) {
            result[*returnSize] = val;
            (*returnSize)++;
            added[val] = true;
        }
    }
    return result;
    
